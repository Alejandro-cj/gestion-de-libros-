<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Books List</title>
</head>
<body>
  <h1>Books List</h1>
  <table border="1">
    <thead>
      <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Publication Date</th>
        <th>ISBN</th>
        <th>Genre</th>
      </tr>
    </thead>
    <tbody>
      <%
      books.forEach(book => { %>
        <tr>
          <td><%= book.title %></td>
          <td><%= book.author %></td>
          <td><%= new Date(book.publicationDate).toLocaleDateString() %></td>
          <td><%= book.isbn %></td>
          <td><%= book.genre %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>